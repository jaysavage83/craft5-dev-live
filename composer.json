{
  "require": {
    "carlcs/craft-diywidget": "*",
    "craftcms/ckeditor": "*",
    "craftcms/cms": "^5.8.17",
    "craftcms/element-api": "*",
    "craftcms/store-hours": "*",
    "craftpulse/craft-timeloop": "*",
    "mmikkel/cp-clearcache": "*",
    "nystudio107/craft-youtubeliveembed": "*",
    "pubnub/pubnub": "*",
    "putyourlightson/craft-sprig": "*",
    "spicyweb/craft-embedded-assets": "*",
    "studioespresso/craft-splashingimages": "*",
    "verbb/navigation": "*",
    "vlucas/phpdotenv": "*",
    "webrgp/craft-ignition": "*"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "*"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.3.6"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}
